<Window x:Class="ResourcesReleaseTool_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResourcesReleaseTool_GUI"
        mc:Ignorable="d"
        Title="明日方舟资源解包软件" Height="648" Width="960" Icon="app_icon.png">
    <Window.Background>
        <ImageBrush ImageSource="background.jpg"/>
    </Window.Background>
    <Grid>
        <Menu Height="40" VerticalAlignment="Top">
            <MenuItem x:Name="___无名称_" Width="111" Click="Button_Click" Height="40" Header="点击载入源文件夹"/>
            <MenuItem Header="点击载入输出文件夹" Height="36" Width="125" Click="Button_Click_1"/>
            <MenuItem Header="开始解包" Height="36" Width="59" Click="Button_Click_2"/>
            <MenuItem x:Name="Cancel" Header="取消操作" Width="59" Height="36" IsEnabled="False" Click="Cancel_Click"/>
            <MenuItem Width="88" Header="Help&amp;About" Click="MenuItem_Click"></MenuItem>
        </Menu>
        <Label x:Name="OutPutLabel" Content="请先载入文件夹" Margin="0,40,0,0" RenderTransformOrigin="0.508,0.493" Background="#7FB1BDB3" Foreground="White"/>
    </Grid>
</Window>
