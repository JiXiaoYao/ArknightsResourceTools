<Window x:Class="ResourcesReleaseTool_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResourcesReleaseTool_GUI"
        mc:Ignorable="d"
        Title="明日方舟资源解包软件" Height="648" Width="960">
    <Window.Background>
        <ImageBrush ImageSource="1032074.jpg"/>
    </Window.Background>
    <Grid>
        <Menu Height="40" VerticalAlignment="Top">
            <Button x:Name="___无名称_" Width="130" Click="Button_Click" Height="30" Content="点击载入源文件夹"/>
            <Button Content="点击载入输出文件夹" Height="30" Width="137" Click="Button_Click_1"/>
            <Button Content="开始解包" Height="30" Width="128" Click="Button_Click_2"/>
            <Button x:Name="Cancel" Content="取消操作" Width="105" Height="30" IsEnabled="False" Click="Cancel_Click"/>
        </Menu>
        <Label x:Name="OutPutLabel" Content="请先载入文件夹" Margin="0,40,0,0" RenderTransformOrigin="0.508,0.493" Background="#7FB1BDB3" Foreground="White"/>
    </Grid>
</Window>
